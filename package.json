{
  "name": "taunus",
  "description": "Micro Isomorphic MVC Engine for Node.js",
  "version": "2.9.3",
  "homepage": "https://github.com/bevacqua/taunus",
  "authors": [
    "Nicolas Bevacqua <nicolasbevacqua@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/bevacqua/taunus.git"
  },
  "bugs": {
    "url": "https://github.com/bevacqua/taunus/issues"
  },
  "main": "lib/index.js",
  "browser": "browser/index.js",
  "bin": {
    "taunus": "bin/taunus"
  },
  "scripts": {
    "lint": "jshint . --reporter node_modules/jshint-stylish/stylish.js",
    "test": "npm run lint",
    "deploy": "npm version ${BUMP:-\"patch\"} --no-git-tag-version && cat package.json | jq .version > version && git add version package.json && git commit -m \"Taunus release v$(cat package.json | jq .version)\" && npm publish && git push",
    "diagnose": "browserify . --full-paths | uglifyjs -m -c | discify --open & browserify . --full-paths | uglifyjs -m -c | gzip-size | pretty-bytes"
  },
  "dependencies": {
    "accepts": "^1.1.1",
    "browserify": "^4.1.5",
    "chalk": "^0.4.0",
    "chokidar": "^0.8.2",
    "contra.emitter": "^1.0.0",
    "fast-url-parser": "^1.0.6-0",
    "glob": "^4.0.6",
    "hget": "^1.0.0",
    "lodash": "^2.4.1",
    "minimist": "^0.1.0",
    "raf": "^2.0.3",
    "routes": "^1.2.0",
    "xhr": "^1.14.1"
  },
  "devDependencies": {
    "disc": "^1.3.0",
    "emoji-random": "^0.1.2",
    "gzip-size": "^1.0.0",
    "jshint": "^2.5.0",
    "jshint-stylish": "^0.2.0",
    "pretty-bytes": "^1.0.1",
    "uglify-js": "^2.4.15"
  }
}
